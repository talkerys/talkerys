model User {
  id           String   @id @default(cuid())
  email        String   @unique
  passwordHash String
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}

model Event {
  id        String   @id @default(cuid())
  title     String
  startsAt  DateTime
  createdAt DateTime @default(now())
}
